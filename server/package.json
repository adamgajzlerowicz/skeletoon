{
	"scripts": {
		"start": "cross-env HASH=nodemon index.js --exec babel-node --presets es2015,stage-2",
		"seed": "nodemon seed.js --exec babel-node --presets es2015,stage-2",
		"lint": "eslint . --ext .js --fix",
		"docker": "docker restart skeletoon",
		"docker-create": "docker run --name skeletoon -p 5432:5432 -e POSTGRES_PASSWORD=dupa -d postgres",
		"docker-create-mysql": "docker run --name skeletoon -e MYSQL_USER=dupa -p 3306:3306 -e MYSQL_ROOT_PASSWORD=dupa -d mysql",
		"docker-destroy": "docker stop skeletoon; docker rm skeletoon"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"babel-register": "^6.26.0",
		"bcrypt": "^2.0.1",
		"cross-env": "^5.1.5",
		"graphql-relay": "^0.5.5",
		"graphql-sequelize": "^8.1.0",
		"graphql-yoga": "1.2.0",
		"jsonwebtoken": "^8.2.1",
		"nodemon": "^1.17.4",
		"pg": "^6.0.0",
		"sequelize": "^4.37.7"
	},
	"devDependencies": {
		"babel-eslint": "^8.2.3",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-dependencies": "^2.4.0",
		"eslint-plugin-flowtype": "^2.46.3",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.8.2"
	}
}
