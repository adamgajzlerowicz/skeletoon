		"bcrypt": "^2.0.1",
		"cross-env": "^5.1.5",
		"graphql": "^0.13.2",
		"graphql-relay": "^0.5.5",
		"graphql-sequelize": "^8.1.0",
		"graphql-tag": "^2.9.2",
		"graphql-yoga": "1.2.0",
		"jsonwebtoken": "^8.2.1",
		"nodemon": "^1.17.4",
		"sequelize": "^4.37.7"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
